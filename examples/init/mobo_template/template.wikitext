<noinclude>==Description==
<templatedata>{{{templatedata}}}</templatedata>
==Usage==
<pre>\{{ {{~name}}
{{#each attr}}|{{this.id}}=
{{/each~}} \}}</pre>
</noinclude>
<includeonly>

{{~#if this.prefix}}{{this.prefix}}{{/if~}}

{{~#if display_table~}}
{| class="wikitable formdata formdata-{{name}}"{{!-- TABLE DISPLAY --}}
! colspan="2"|{{name}}
{{#each attr}}
|- \{{#if: \{{{ {{~this.id~}} |}}} | | style="display: none;" \}} {{~#if this.smw_hideInPage}} style="display: none;"{{~/if}}
| '''{{this.title}}'''
| <span data-property="{{this.id}}" data-value="{{smw_templ_attr this}}">{{smw_templ_attr this}}</span>
{{/each}}
|-
|}
{{~/if~}}


{{~#if display_bootstrap-table~}}
{| class="formdata formdata-{{name}} table table-bordered table-hover table-condensed"{{!-- TABLE DISPLAY --}}
! colspan="2"|{{name}}
{{#each attr}}
|- \{{#if: \{{{ {{~this.id~}} |}}} | | style="display: none;" \}} {{~#if this.smw_hideInPage}} style="display: none;"{{~/if}}
| '''{{this.title}}'''
| <span data-property="{{this.id}}" data-value="{{smw_templ_attr this}}">{{smw_templ_attr this}}</span>
{{/each}}
|-
|}
{{~/if~}}



{{~#if display_ul~}}{{!-- UL (UNORDERED LIST) DISPLAY --}}
<div class="wiki-ul formdata formdata-{{name}}">
{{~#each attr~}}
\{{#if: \{{{ {{~@key~}} |}}} | * '''{{this.title}}:''' <span data-property="{{this.id}}" data-value="{{smw_templ_attr this}}">{{smw_templ_attr this}}</span> |}}
{{~/each~}}
</div>
{{~/if~}}


{{~#if display_hidden~}}{{!-- HIDDEN DISPLAY --}}
<div class="wiki-hidden formdata formdata-{{name}}" style="display: none;">
{{~#each attr~}}
\{{#if: \{{{ {{~@key~}} |}}} | * '''{{this.title}}:''' <span data-property="{{this.id}}" data-value="{{smw_templ_attr this}}">{{smw_templ_attr this}}</span> |}}
{{~/each~}}
</div>
{{~/if~}}


{{~#if display_none~}}{{!-- NONE DISPLAY --}}{{~/if~}}


{{~#if this.postfix}}{{this.postfix}}{{/if~}}

{{~#if smw_subobject~}}
{{~smw_subobject attr name~}}
{{~else~}}
{{smw_set attr}}
{{~/if~}}

{{~#if category~}}[[category:{{category}}]]{{~/if~}}
{{~#if categories~}}{{#each categories~}}[[category:{{this}}]]{{/each~}}{{/if~}}</includeonly>
