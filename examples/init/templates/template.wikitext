<noinclude>==Description==
<templatedata>{{{templatedata}}}</templatedata>
==Usage==
<pre>\{{ {{~name}}
{{#each attr}}|{{@key}}=
{{/each~}} \}}</pre>
</noinclude>
<includeonly>


{{!-- TABLE DISPLAY --}}
{{~#if display_table~}}
{| class="wikitable formdata"
! colspan="2"|{{name}}
{{#each attr}}
|- \{{#if: \{{{ {{~@key~}} |}}} | | style="display: none;" \}}
| '''{{this.title}}'''
{{#if this.smw_arraymap~}}
| \{{#arraymap:\{{{ {{~@key~}} |}}}|;|x|[[x]]}}
{{else~}}
| [[\{{{ {{~@key~}} |}}}]]
{{/if~}}
{{/each}}
|-
|}
{{~/if~}}

{{!-- UL (UNORDERED LIST) DISPLAY --}}
{{~#if display_ul~}}
==={{name}}===
{{#each attr~}}
{{~#if this.smw_arraymap~}}
\{{#if: \{{{ {{~@key~}} |}}} | * '''{{this.title}}''': \{{#arraymap:\{{{ {{~@key~}} |}}}|;|x|[[x]]}} |}}
{{else~}}
\{{#if: \{{{ {{~@key~}} |}}} | * '''{{this.title}}''': \{{{ {{~@key~}} |}}} |}}
{{/if~}}
{{~/each}}
{{~/if~}}


{{~#if smw_subobject~}}
{{smw_subobject attr}}
{{~else~}}
{{smw_set attr}}
{{~/if~}}

{{~#if category~}}[[category:{{category}}]]{{~/if~}}
{{~#if categories~}}{{#each categories~}}[[category:{{this}}]]{{/each~}}{{/if~}}</includeonly>
