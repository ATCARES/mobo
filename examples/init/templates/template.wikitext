<noinclude>==Description==
<templatedata>{{{templatedata}}}</templatedata>
==Usage==
<pre>\{{ {{~name}}
{{#each attr}}|{{@key}}=
{{/each~}} \}}</pre>
</noinclude>
<includeonly>


{{~#if display_table~}}{{!-- TABLE DISPLAY --}}
{| class="wikitable formdata"
! colspan="2"|{{name}}
{{#each attr}}
|- \{{#if: \{{{ {{~@key~}} |}}} | | style="display: none;" \}}
| '''{{this.title}}'''
| {{smw_templ_attr this }}
{{/each}}
|-
|}
{{~/if~}}


{{~#if display_ul~}}{{!-- UL (UNORDERED LIST) DISPLAY --}}
==={{name}}===
{{#each attr~}}
\{{#if: \{{{ {{~@key~}} |}}} | * '''{{this.title}}''': {{smw_templ_attr this}} |}}
{{~/each}}
{{~/if~}}


{{~#if smw_subobject~}}
{{smw_subobject attr}}
{{~else~}}
{{smw_set attr}}
{{~/if~}}

{{~#if category~}}[[category:{{category}}]]{{~/if~}}
{{~#if categories~}}{{#each categories~}}[[category:{{this}}]]{{/each~}}{{/if~}}</includeonly>
