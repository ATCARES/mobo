<noinclude>==Description==
<templatedata>{{{templatedata}}}</templatedata>
==Usage==
<pre>\{{ {{~name}}
{{#each attr}}|{{@key}}=
{{/each~}} \}}</pre>
</noinclude>
<includeonly>

{{!-- TABLE DISPLAY --}}
{{~#if display_table~}}
{| class="wikitable formdata"
! colspan="2"|{{name}}
|-
{{#each attr}}
| '''{{this.title}}'''
| \{{{ {{~@key~}} |}}}
|-
{{/each}}
|}
{{~/if~}}

{{!-- UL (UNORDERED LIST) DISPLAY --}}
{{~#if display_ul~}}
==={{name}}===
{{#each attr~}}
* '''{{this.title}}''': \{{{ {{~@key~}} |}}}
{{/each}}
{{~/if~}}

{{~#if smw_subobject~}}
\{{#subobject:
{{#each attr~}}
    | {{@key}}=\{{{ {{~@key}} |}}}
{{/each~}}
}}
{{~else~}}
{{smw_set attr}}
{{~/if~}}

{{~#if category~}}[[category:{{category}}]]{{~/if~}}
{{~#if categories~}}{{#each categories~}}[[category:{{this}}]]{{/each~}}{{/if~}}</includeonly>
